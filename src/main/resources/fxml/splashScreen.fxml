<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<FlowPane
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="com.example.ssp.controllers.SplashScreen"
        prefHeight="600.0"
        prefWidth="600.0"
        alignment="CENTER">

    <ImageView
            id="splash"
            onMouseClicked="#splashButtonClicked">

        <Image url="@/images/splashscreen.jpg"
               requestedHeight="600"
               requestedWidth="600"/>

    </ImageView>

</FlowPane>
